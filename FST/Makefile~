all: fst.out

sbg/sbg.out:  sbg/sbg.f sbg/flx.f 
	f77 -O3 -o sbg/sbg.out -C -w sbg/sbg.f sbg/flx.f -lnag

sbg/sbr.f: sbg/sbg.out sbg/sbg.beg
	cd sbg; wrun sbg; cd ..

fst.out:  fst.f sbg/sbr.f cnd.f sbg/flx.f
	f77 -O3 -o fst.out -C -w fst.f sbg/sbr.f cnd.f sbg/flx.f -lnag

clean:
	rm -f *.out sbg/*.out
